qemu-system-riscv64 -machine virt  -bios bootloader/rustsbi-qemu.bin -device loader,file=os/target/riscv64gc-unknown-none-elf/release/os,addr=0x80200000 -drive file=user/target/riscv64gc-unknown-none-elf/release/fs.img,if=none,format=raw,id=x0 -device virtio-blk-device,drive=x0,bus=virtio-mmio-bus.0